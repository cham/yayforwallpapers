{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "no-param-reassign": ["off"],
        "no-prototype-builtins": ["off"],
        "global-require": ["off"],
        "no-console": ["off"],
        "consistent-return": ["off"],
        "no-shadow": ["off"],
        "comma-dangle": ["off"]
    },
    "globals": {
      "describe": true,
      "before": true,
      "beforeEach": true,
      "after": true,
      "afterEach": true,
      "it": true
    }
}
